{
    "name": "@bitmain/dnd-grid",
    "version": "0.0.1",
    "description": "A vuejs grid with draggable and resizable boxes",
    "author": "xueyan.lu <xueyan.lu@bitmain.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "fork": "git+https://github.com/dattn/dnd-grid.git",
        "url": "git+https://github.com/bitmain-frontend/dnd-grid.git"
    },
    "main": "dist/dnd-grid.js",
    "scripts": {
        "dist": "poi build ./src/index.js --dist ./dist/ --prod --library",
        "dist:demo": "poi build ./src/demo/index.js --dist ./demo/ --prod",
        "build": "npm run dist -- --minimize 0",
        "build:demo": "npm run dist:demo -- --minimize 0",
        "start": "npm run dev",
        "dev": "poi ./src/demo/index.js",
        "lint": "eslint --ext .js,.vue src"
    },
    "files": [
        "dist/"
    ],
    "keywords": [
        "vue",
        "component"
    ],
    "devDependencies": {
        "babel-eslint": "^8.2.2",
        "eslint": "^4.19.1",
        "eslint-config-standard": "^11.0.0",
        "eslint-plugin-html": "^4.0.2",
        "eslint-plugin-import": "^2.7.0",
        "eslint-plugin-node": "^6.0.1",
        "eslint-plugin-promise": "^3.7.0",
        "eslint-plugin-standard": "^3.0.1",
        "poi": "^9.6.13"
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 10 versions",
        "not ie <= 8"
    ]
}
